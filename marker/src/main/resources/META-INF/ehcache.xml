<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="false">
	<!-- <diskStore path="Users/dec7/apps/caches" /> -->
	<!-- <diskStore path="${cache.basedir}" /> -->
	<diskStore path="$HOME/caches" />

	<defaultCache eternal="false" overflowToDisk="false"
		maxElementsInMemory="50000" timeToIdleSeconds="30" timeToLiveSeconds="6000"
		memoryStoreEvictionPolicy="LRU" />
	
	<cache name="role" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" />
	<cache name="user" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" />
	<!-- <cache name="member" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" />
	<cache name="program" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" />
	<cache name="urlRepository" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" />
	<cache name="urlRole" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" />
	<cache name="userRole" maxElementsInMemory="1000" timeToIdleSeconds="3600" timeToLiveSeconds="3600" /> -->
	
</ehcache>